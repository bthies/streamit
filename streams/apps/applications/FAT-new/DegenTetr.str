complex->complex[N_bm][N_srg][N_dop][N_stag] filter DegenTetr(int N_bm,int N_srg,int N_dop,N_stag) {
    init {}
    work pop N_bm*N_srg*N_dop*N_stag push 1 {
	complex[N_bm][N_srg][N_dop][N_stag] InArr;

	InArr=pop();
	
	for (int i=0; i<N_bm; i++)
	    for (int j=0; j < N_srg; j++)
		for (int k=0; k < N_dop; k++)
		    for(int l=0; l < N_stag;l++)
			push (InArr[i][j][k][l]);
    }
}
















