void->void pipeline Partition1 {
    add Source();
    add splitjoin {
	split duplicate;
	for (int i=0; i<10; i++) {
	    add pipeline {
		for (int j=0; j<10; j++) {
		    add Incr();
		}
	    }
	}
	join roundrobin;
    }
    add Printer();
}

void->int filter Source {
    int i = 0;
    work push 1 {
	push(i++);
    }
}

int->int filter Incr {
    work push 1 pop 1 {
	push(pop()+1);
    }
}

int->void filter Printer {
    work pop 1 {
	print(pop());
    }
}
